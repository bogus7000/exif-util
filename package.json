{
  "name": "exif-util",
  "description": "CLI for inspecting and comparing exif tags",
  "version": "0.0.2-alpha",
  "author": "kbd-overlord",
  "bin": {
    "exif-util": "./bin/run"
  },
  "bugs": "https://github.com/kbd-overlord/exif-util/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.7",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/chalk": "^2.2.0",
    "@types/inquirer-fuzzy-path": "^2.3.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/signale": "^1.4.2",
    "cfonts": "^2.10.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1.2",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kbd-overlord/exif-util",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "exif-util",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kbd-overlord/exif-util",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.7",
    "@oclif/config": "^1.18.1",
    "@oclif/plugin-help": "^3.2.10",
    "@types/inquirer": "^8.1.3",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.0",
    "inquirer-fuzzy-path": "^2.3.0",
    "signale": "^1.4.0",
    "ts-exif-parser": "^0.2.2",
    "tslib": "^1.14.1"
  }
}
